{
    "require": {
        "coenjacobs/mozart": "*",
        "jay-a-j-wilson/jwws-wp-plugin-framework": "3.1.0"
    },
    "require-dev": {
        "yoast/phpunit-polyfills": "^1.0",
        "phpunit/phpunit": "^9.6"
    },
    "autoload": {
        "psr-4": {
            "JWWS\\ACA\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JWWS\\ACA\\Tests\\": "tests/"
        }
    },
    "extra": {
        "mozart": {
            "dep_namespace": "JWWS\\ACA\\Deps\\",
            "dep_directory": "/src/Deps/",
            "classmap_directory": "/src/Deps/classes/",
            "classmap_prefix": "JWWS_ACA_",
            "packages": [
                "jay-a-j-wilson/jwws-wp-plugin-framework"
            ],
            "delete_vendor_directories": true
        }
    },
    "scripts": {
        "post-install-cmd": [
            "\"vendor/bin/mozart\" compose",
            "composer dump-autoload"
        ],
        "post-update-cmd": [
            "\"vendor/bin/mozart\" compose",
            "composer dump-autoload"
        ]
    }
}
